#include <stdio.h>
#include <math.h>
int main()
{
    int n;
    scanf("%d",&n);
    int flag=0;
    for(int i=0;i<n;i++){
        if(n==i*i){
            printf("Perfect Square");
            flag=1;
            break;
        }
        else{
            flag=0;
        }
    }
    if(flag==0){
        printf("Not a Perfect Square");
    }
    return 0;
}

