#include <stdio.h>
#include <math.h>
int main()
{
    long long int a;
    scanf("%lld",&a);
    long long int temp=a;
    int rev=0;
    while(a>0){
        rev=rev*10 + a%10;
        a=a/10;
    }
    if(rev==temp){
        printf("It is Palindrome");
    }
    else{
        printf("Not a Palindrome");
    }
    return 0;
}
