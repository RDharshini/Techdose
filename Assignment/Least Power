#include <stdio.h>
int Power1(int n){
    int c=0;
    while(n && n&(n-1)){
        n=n&(n-1);
    }
    return n;
}
int Power2(int n){
    int c=0;
    while(n!=1){
        n>>1;
        c++;
    }
    return 1<<c;
}
int main() {
    int n;
    scanf("%d",&n);
    printf("%d %d",Power1(n),Power2(n));
    
    return 0;
}
