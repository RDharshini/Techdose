class Solution {
    public int findKthLargest(int[] nums, int k) {
        int n=nums.length;
       /* //int l=;
        for(int i=0;i<n-1;i++){
            for(int j=i+1;j<n;j++){
                if(nums[i]>nums[j]){
                    int temp=nums[i];
                    nums[i]=nums[j];
                    nums[j]=temp;
                }
            }
        }*/
        Arrays.sort(nums);
    
        return nums[n-k];
    }
}
