class Solution {
    public int percentageLetter(String s, char letter) {
        int n=s.length();
        int count=0;
        int m=0;
        for(char c:s.toCharArray()){
            if(c==letter){
                count++;
            }
        }
        if(count==0){
            m=0;
        }
        else{
             m= (count*100)/n;
        }
        return m;
    }
}
